const rem = (nums)=> {
    for(let i =0; i < nums.length; i++) {
        for(j=i+1; j<nums.length; j++) {
            if(nums[i] == nums[j]) {
                nums.splice(j, 1, '_')
            }
        }
    }
    nums.sort()
    let count = 0
    nums.forEach(item=> {
        if(item!=='_') {
            count++
        }
    })
    return `${count}, nums = [${nums}]`
        }
    
console.log(rem([0,0,1,1,1,2,2,3,3,1]))    
    
// [0,0,1,1,1,2,2,3,3,4]

